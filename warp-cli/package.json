{
  "name": "warp-cli",
  "version": "1.0.0",
  "description": "A standalone CLI-based coding agent with support for local and cloud LLMs",
  "main": "dist/index.js",
  "bin": {
    "warp": "./dist/cli.js",
    "warp-cli": "./dist/cli.js"
  },
  "keywords": [
    "cli",
    "ai",
    "coding-agent",
    "llm",
    "terminal",
    "warp",
    "assistant"
  ],
  "author": "AI CLI Team",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "watch": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "install-global": "npm run build && npm link",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "conf": "^10.2.0",
    "inquirer": "^8.2.5",
    "node-fetch": "^2.7.0",
    "openai": "^4.20.1",
    "ora": "^5.4.1",
    "simple-git": "^3.28.0",
    "sqlite3": "^5.1.7",
    "strip-ansi": "^6.0.1",
    "boxen": "^5.1.2",
    "cli-table3": "^0.6.3",
    "marked": "^4.3.0",
    "marked-terminal": "^5.2.0",
    "dotenv": "^16.0.3",
    "uuid": "^9.0.0",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.5",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.19.0",
    "@types/node-fetch": "^2.6.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
