{
  "name": "tabby-ai-agent",
  "version": "1.0.0",
  "description": "Warp-style AI terminal integration for Tabby with MCP support",
  "main": "dist/index.js",
  "keywords": [
    "tabby",
    "terminal",
    "ai",
    "mcp",
    "warp"
  ],
  "author": "Your Name",
  "license": "MIT",
  "scripts": {
    "build": "tsc && webpack",
    "dev": "webpack serve --mode development",
    "dev:watch": "tsc --watch",
    "install-mcp": "node scripts/install-mcp-servers.js",
    "setup": "npm run install-mcp && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "check": "npm run lint && npm run test && npm run build"
  },
  "dependencies": {
    "@angular/common": "^9.1.13",
    "@angular/core": "^9.1.13",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "node-fetch": "^3.3.0",
    "rxjs": "^6.5.3",
    "sanitize-html": "^2.11.0",
    "simple-git": "^3.28.0",
    "tabby-core": "^1.0.0",
    "tabby-terminal": "^1.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^18.0.0",
    "@types/sanitize-html": "^2.9.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.0",
    "jest": "^30.0.5",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "typescript": "^4.9.5",
    "webpack": "^5.101.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.0"
  },
  "tabbyPlugin": {
    "name": "ai-agent",
    "displayName": "AI Agent Terminal",
    "description": "Warp-style AI assistant with MCP tools integration",
    "homepage": "https://github.com/yourusername/tabby-ai-agent",
    "configSchema": "./config-schema.json"
  }
}
