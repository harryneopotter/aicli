{
  "name": "aicli",
  "version": "1.0.0",
  "description": "A standalone CLI-based coding agent with support for local and cloud LLMs",
  "main": "dist/index.js",
  "bin": {
    "aicli": "dist/cli.js"
  },
  "keywords": [
    "cli",
    "ai",
    "coding-agent",
    "llm",
    "terminal",
    "aicli",
    "assistant"
  ],
  "author": "AI CLI Team",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "watch": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "install-global": "npm run build && npm link",
    "clean": "rmdir /s dist 2>nul || rm -rf dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@types/keytar": "^4.4.0",
    "better-sqlite3": "^10.1.0",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "conf": "^10.2.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "google-it": "^1.6.4",
    "gradient-string": "^2.0.2",
    "gray-matter": "^4.0.3",
    "inquirer": "^8.2.5",
    "keytar": "^7.9.0",
    "marked": "^4.3.0",
    "marked-terminal": "^5.2.0",
    "node-fetch": "^2.7.0",
    "openai": "^4.20.1",
    "ora": "^5.4.1",
    "simple-git": "^3.28.0",
    "strip-ansi": "^6.0.1",
    "ts-morph": "^27.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/eslint": "^8.56.10",
    "@types/inquirer": "^8.2.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.0",
    "@types/node-fetch": "^2.6.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-plugin-security": "^2.1.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
